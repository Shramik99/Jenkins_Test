pipeline {
    agent any
    
    stages {
        stage('Stage 1') {
            steps {
                echo 'Hello From GitHub!!'
            }
        }
        stage('Stage 2'){
            steps{
                echo 'hello from github'
            }
        }
        stage('Stage 3'){
            steps{
                echo 'Hello from Step 3s'
            }
        }
        stage('Stage 4'){
            steps{
                echo 'Hello from dev file'
            }
        }
    }
}
